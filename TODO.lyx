\end_inset

.
\end_layout

\begin_layout Paragraph
求
\begin_inset Formula 
\[
\lim_{n\to\infty}\sum_{i=1}^{n}\sum_{j=1}^{n}\sum_{k=1}^{n}\sum_{h=1}^{n}\frac{n^{6}}{\left(n+i\right)\left(n^{2}+j^{2}\right)\left(n^{3}+k^{3}\right)\left(n^{4}+h^{4}\right)}=\frac{\pi\ln2}{4}\left(\frac{\sqrt{3}\pi}{9}+\frac{\ln2}{3}\right)\left(\frac{\pi}{4\sqrt{2}}+\frac{\ln\left(\sqrt{2}+1\right)}{2\sqrt{2}}\right).
\]

\end_inset


\begin_inset Formula 
\[
\lim_{n\to\infty}n^{2}\left[\sum_{i=1}^{n}\frac{2\ln\left(n+i\right)-2\ln n}{n^{2}+i^{2}}-\sum_{j=1}^{n}\sum_{k=1}^{n}\frac{1}{\left(n+j\right)\left(n^{2}+k^{2}\right)}\right]=\frac{12\ln2+\pi}{16}.
\]

\end_inset


\begin_inset Formula 
\[
\lim_{x\to0}\frac{\ln\ln\ln\left[x+\left(1+x\right)^{\left(1+x\right)^{1/x}/x}\right]+x\left(1-\frac{1}{\ue^{\ue+1}}\right)}{x^{2}}=\frac{1}{2}\ue+\frac{25}{24}-\frac{\left(1+\ue\right)\left(1-\ue^{\ue+1}\right)^{2}}{2\ue^{2\ue+2}}.
\]

\end_inset


\begin_inset Formula 
\[
\lim_{n\to\infty}n^{2}\left(-2\pi+n\sin2\pi\ue n!\right)=-2\pi.
\]

\end_inset


\begin_inset Formula 
\[
\lim_{n\to\infty}n\left[\sqrt[n]{1^{n}+2^{n}+\cdots+n^{n}}-n-1+\ln\left(\ue-1\right)\right]=-\frac{\ue+1}{2\left(\ue-1\right)^{2}}-\frac{\left[\ln\left(\ue-1\right)-1\right]^{2}}{2}.
\]

\end_inset


\end_layout

\begin_layout Paragraph
求
