\]

\end_inset


\end_layout

\begin_layout Paragraph
求
\begin_inset Formula 
\[
\lim_{x\to0}\frac{\tan^{5/2}x-\ln\left(1+\sin^{5/2}x\right)}{\left[\ln\left(x+\sqrt{1+x^{2}}\right)-\sin x\right]^{9/10}}=\frac{5}{4}\times15^{9/10}.
\]

\end_inset


\end_layout

\begin_layout Paragraph
求
\begin_inset Formula 
\[
\lim_{x\to1^{-}}\left(\frac{4\arctan\frac{1+x}{1-x}}{\left(x-1\right)^{3}}+\frac{x-3}{\left(x-1\right)^{2}}-\frac{2\pi}{\left(x-1\right)^{3}}\right)=\frac{1}{3}.
\]

\end_inset


\end_layout

\begin_layout Paragraph
求
\begin_inset Formula 
\[
\lim_{n\to\infty}\sum_{k=1}^{n}\frac{k\int_{0}^{k\pi}x\left|\sin x\right|\ud x}{\pi\ue^{k}}=\frac{\ue^{3}+4\ue^{2}+\ue}{\left(\ue-1\right)^{4}}.
\]

\end_inset


\end_layout

\begin_layout Paragraph
求
\begin_inset Formula 
\[
\lim_{x\to0}\frac{\left(\cos x\right)^{\cos x}-\left(1-x\right)^{x}-\frac{1}{2}x^{2}}{\arcsin\tan x-\arctan\arctan x}=\frac{3}{7}.
\]

\end_inset


\begin_inset Formula 
\[
\lim_{n\to\infty}\sum_{k=0}^{n}\frac{2^{3k}\cdot3^{-3k/2}}{\left(3k\right)!}=\frac{1}{3}\ue^{2/\sqrt{3}}+\frac{2}{3}\ue^{-1/\sqrt{3}}\cos1-1.
\]

\end_inset


\begin_inset Formula 
\[
\lim_{x\to0}\frac{\sum_{k=1}^{n-1}\sqrt[k]{\cos kx}-\left(n-1\right)\sqrt[n]{\cos nx}}{1-\prod_{k=1}^{n}\cos^{k}\left(n-k+1\right)x}=\frac{6\left(n-1\right)}{\left(n+1\right)^{2}\left(n+2\right)}.
\]

\end_inset


\begin_inset Formula 
\[
\lim_{n,m\to\infty}\sum_{i=1}^{n}\sum_{j=1}^{m}\frac{\left(-1\right)^{i+j}}{i+j}=\ln2-\frac{1}{2}.
\]

\end_inset


\begin_inset Formula 
\[
\lim_{n\to\infty}\frac{\pi^{n}n^{n}}{\left(2+\sum_{r=1}^{n}\frac{4^{r}\left(r!\right)^{2}}{\left(2r+1\right)!}\right)^{2n}}=\ue^{-5/4}.
\]

\end_inset

提示: 用Stolz公式估计分母中括号内的阶为
\begin_inset Formula $\sqrt{n}$
\end_inset

, 或者
\begin_inset Formula 
\[
\sum_{r=1}^{n}\frac{4^{r}\left(r!\right)^{2}}{\left(2r+1\right)!}=\frac{4^{1+n}\left[\left(n+1\right)!\right]^{2}}{\left(2n+2\right)!}-2.
\]

\end_inset

可以证明
\begin_inset Formula 
\[
2+\sum_{r=1}^{n}\frac{4^{r}\left(r!\right)^{2}}{\left(2r+1\right)!}=\sqrt{\pi n}+\frac{5\sqrt{\pi}}{32\sqrt{n}}+o\left(\frac{1}{n}\right).
\]

\end_inset


