\begin_body

\begin_layout Section
Stokes算子
\end_layout

\begin_layout Standard
有界区域上的Stokes算子的谱
\end_layout

\begin_layout Standard
\begin_inset Formula $\mathbb{P}:L^{2}(\Omega)^{n}\to H$
\end_inset

为正交投影算子.
\end_layout

\begin_layout Standard
\begin_inset Formula $\Omega$
\end_inset

: 有界区域, 
\begin_inset Formula $\partial\Omega$
\end_inset

是
\begin_inset Formula $C^{2}$
\end_inset

类.
\end_layout

\begin_layout Standard
\begin_inset Formula $H=L_{\sigma}^{2}$
\end_inset

.
\end_layout

\begin_layout Standard
Def.
 Stokes 算子 
\begin_inset Formula $A$
\end_inset

 的定义 
\begin_inset Formula 
\[
A:D(A)\to H,
\]

\end_inset

其中
\begin_inset Formula $D(A)=H^{2}(\Omega)\cap V\subset H$
\end_inset

, 
\begin_inset Formula $A\coloneqq-\mathbb{P}\Delta$
\end_inset

.
\end_layout

\begin_layout Standard
Prop.
 Stokes算子是对称算子, 即
\begin_inset Formula 
\[
(Au,v)=(u,Av),\qquad\forall u,v\in D(A).
\]

\end_inset


\end_layout

\begin_layout Standard
pf.
 
\end_layout

\begin_layout Standard

\end_layout

\end_body
